    "license": "MIT",
    "scripts": {
        "pre-build": "rimraf dist",
        "build": "tsc -p tsconfig.prod.json  && tsc-alias",
        "dep:check": "depcruise --config .dependency-cruiser.cjs src",
        "dep:paint": "depcruise --include-only '^src' --output-type dot src | dot -T svg > dependencygraph.svg",
        "dev": "nodemon",
        "lint": "eslint . --ext .ts",
        "start": "node dist/server.js",
        "test": "jest --detectOpenHandles",
        "test:watch": "jest --watch",
        "prepare": "husky"
    },
    "devDependencies": {
        "@eslint/js": "^9.5.0",
        "@jest/globals": "^29.7.0",
        "@types/bcrypt": "^5.0.2",
        "@types/body-parser": "^1.19.5",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.14.5",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^7.13.1",
        "@typescript-eslint/parser": "^7.13.1",
        "cpu-features": "^0.0.10",
        "dependency-cruiser": "^16.3.3",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "globals": "^15.6.0",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.7",
        "nodemon": "^3.1.3",
        "rimraf": "^5.0.7",
        "supertest": "^7.0.0",
        "testcontainers": "^10.9.0",
        "ts-alias": "^0.0.7",
        "ts-jest": "^29.1.5",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "tsc-alias": "^1.8.10",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.5.2",
        "typescript-eslint": "^7.13.1"
    },
    "dependencies": {
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "express": "^4.19.2",
        "express-rate-limit": "^7.3.1",
        "helmet": "^7.1.0",
        "prettier": "^3.3.2"
    },
    "lint-staged": {
        "*.(js|ts)": [
            "eslint --fix",
            "prettier --write"
        ]
    }
}
